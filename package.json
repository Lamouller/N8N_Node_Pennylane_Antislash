{
  "name": "n8n-nodes-pennylane-antislash",
  "version": "1.1.0",
  "description": "n8n community node for Pennylane External API v2",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "pennylane",
    "accounting",
    "invoicing",
    "finance"
  ],
  "license": "MIT",
  "homepage": "https://n8n.io",
  "author": {
    "name": "antislash",
    "email": "contact@antislash.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lamouller/N8N_Node_Pennylane_Antislash.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier src --write",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "nodes",
    "triggers"
  ],
  "n8n": {
    "n8nNodesApiVersion": 2,
    "credentials": [
      "dist/credentials/PennylaneTokenApi.credentials.js",
      "dist/credentials/PennylaneOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Pennylane/Pennylane.node.js",
      "dist/triggers/PennylaneTrigger/PennylaneTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^29.0.0",
    "@types/node": "^16.18.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  }
}
